<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
        version="4.0">
  <servlet>
    <servlet-name>downfile</servlet-name>
    <servlet-class>com.mihoya.studyResponse.DownloadFile</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>downfile</servlet-name>
    <url-pattern>/down</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>image</servlet-name>
    <servlet-class>com.mihoya.studyResponse.imageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>image</servlet-name>
    <url-pattern>/image</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>redirect</servlet-name>
    <servlet-class>com.mihoya.studyResponse.RediServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>redirect</servlet-name>
    <url-pattern>/red</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>rediLogin</servlet-name>
    <servlet-class>com.mihoya.studyResponse.RediLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rediLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <display-name>Archetype Created Web Application</display-name>
</web-app>
